<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="gvn_ab_mobile.Controls.PickerSearch">
    <ContentView.Content>
        <StackLayout x:Name="LabelItems" HeightRequest="30">
            <Label BackgroundColor="Transparent" FontSize="Small" VerticalOptions="CenterAndExpand" HorizontalOptions="Fill" TextColor="Gray" Text="{Binding Title}" />
        </StackLayout>
    </ContentView.Content>
</ContentView>