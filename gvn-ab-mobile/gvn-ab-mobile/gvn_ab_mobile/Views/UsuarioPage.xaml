<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="gvn_ab_mobile.Views.UsuarioPage"
             Title="{Binding Title}">
	<StackLayout Spacing="20" Padding="15">
        <Label Text="{Binding Usuario.Id}" FontSize="Small"/>
        <Label Text="{Binding Usuario.Cpf}" FontSize="Small"/>
        <Label Text="{Binding Usuario.Nome}" FontSize="Small"/>
        <Label Text="{Binding Usuario.Password}" FontSize="Small"/>
    </StackLayout>
</ContentPage>